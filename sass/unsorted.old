

#fpmenu input[type="submit"] {
    width: 80px;
}

#fpmenu select {
    margin: 5px 10px;
    width: 500px;
}

#fpmenu select:last-child {
    margin-bottom: 0;
}

#fwmenu {
    background: #FFFFE7;
    border: 1px solid #278727;
    font-size: 100%;
    font-weight: 400;
    max-width: 600px;
    position: absolute;
    text-align: left;
    z-index: 1005;
}

#right {
    bottom: 5px;
    left: 180px;
    position: absolute;
    right: 5px;
    top: 20px;
}

.col2 {
    text-align: center;
}

.colorpicker_ct .slider {
    background: url(../jscolor/ct_background.svg);
}

.colorpicker_ct_mired .slider {
    background: url(../jscolor/ct_mired_background.svg);
}

.colorpicker_hue .slider {
    background: url(../jscolor/hue_background.svg);
}

.dashboard_widgetheader svg {
    margin-right: 5px;
}

.devType {
    padding-top: 20px;
}

.downText {
    margin-top: 2px;
}

.evo-cHist {
    padding: 3px;
}

.evo-cHist div {
    border: solid 1px silver;
    cursor: pointer;
    float: left;
    height: 10px;
    margin: 5px;
    padding: 3px;
    width: 10px;
}

.evo-color {
    padding: 1px 3px 0 4px;
    width: 94px;
}

.evo-color div {
    border: solid 1px gray;
    border-bottom: solid 1px silver;
    border-right: solid 1px silver;
    float: left;
    height: 10px;
    margin-bottom: 5px;
    padding: 3px;
    width: 10px;
}

.evo-color span {
    float: left;
    font-size: 15px;
    margin: 1px 0 4px 3px;
}

.evo-colorbox-ie {
    font-size: 8px;
    padding: 3px 9px !important;
}

.evo-colorind {
    position: relative;
    top: 2px;
}

.evo-colorind, .evo-colorind-ie, .evo-colorind-ff {
    border: solid 1px #c3c3c3;
    float: right;
    height: 20px;
    margin-top: 3px;
    width: 20px;
}

.evo-colorind-ie {
    position: relative;
    top: -23px;
}

.evo-colortxt-ie {
    position: relative;
    top: -6px;
}

.evo-more {
    font-size: smaller;
    padding: 4px 5px;
}

.evo-palcenter {
    padding: 5px;
    text-align: center;
}

.evo-palette div.sep {
    height: 3px;
}

.evo-palette td, .evo-palette-ie td {
    border: solid 1px silver;
    cursor: pointer;
    font-size: 1px;
    padding: 7px;
}

.evo-palette th, .evo-palette-ie th {
    border: 0;
    font-weight: 400;
    padding: 5px 3px;
    text-align: left;
}

.evo-palette tr.bottom td {
    border-top: 0;
}

.evo-palette tr.in td {
    border-bottom: 0;
    border-top: 0;
}

.evo-palette tr.top td {
    border-bottom: 0;
}

.evo-palette, .evo-palette-ie {
    border-collapse: expression(separate',cellSpacing='2px);
    border-spacing: 4px 0;
}

.evo-palette2 td {
    padding: 6px 7px;
}

.evo-palette2 td, .evo-palette2-ie td {
    cursor: pointer;
    font-size: 1px;
}

.evo-palette2, .evo-palette2-ie {
    border-collapse: collapse;
    margin: auto;
}

.evo-palette2-ie td, table.block td > div, table.block td > a {
    padding: 5px;
}

.evo-pointer {
    cursor: pointer;
    position: absolute;
    visibility: hidden;
}

.evo-pop {
    -index: 10000;
    border: 1px solid;
    padding: 3px 3px 0;
    width: 204px;
}

.evo-pop-ie {
    padding: 3px;
    width: 212px;
    z-index: 10000;
}

.evo-pop:after, .evo-pop-ie:after, .evo-colorind:after, .evo-colorind-ie:after, .evo-colorind-ff:after, .evo-color span:after, .evo-cHist:after {
    clear: both;
    content: ".";
    display: block;
    font-size: 0;
    height: 0;
    visibility: hidden;
}

.evo-sep {
    font-size: 0;
    height: 10px;
}

.rc_body {
    background: #C8C8B0;
    border-color: gray;
    border-style: solid;
    border-width: 2px;
    font-size: 6px;
    padding: 5px;
}

.rc_button {
    padding: 5px 7px;
}

.rc_button img {
    border-color: transparent;
    border-style: solid;
    border-width: 1px;
}

.rc_button img:active {
    border-color: gray;
}

.set .set {
    margin-bottom: 2px;
    margin-top: 3px;
}



.ui-button-text {
    color: #555 !important;
    font-weight: 400 !important;
}

.ui-corner-bottom, .ui-corner-top, .ui-corner-left, .ui-corner-right, .ui-corner-all, #dashboard .block.wide {
    border-radius: 0 !important;
}

.ui-state-highlight {
    height: 1.1em;
    line-height: 1.1em;
}

.ui-state-hover {
    background: #fff !important;
    border: 1px solid $primary-color !important;
    cursor: pointer;
}

.ui-widget {
    font-family: Arial, sans-serif !important;
}

.ui-widget-overlay {
    filter: Alpha(Opacity=30);
    height: 100%;
    left: 0;
    opacity: .7;
    position: fixed;
    top: 0;
    width: 100%;
}

a.evo-hist {
    margin-left: 6px;
}

div#svgmarker {
    background: #FFFFE7;
    border: 2px solid #278727;
    border-radius: 4px;
    color: #278727;
    max-width: 600px;
    padding: 6px 10px;
    position: absolute;
    text-align: left;
    z-index: 1005;
}

div.block {
    background: #F8F8E0;
    border: 1px solid gray;
    padding: .7em;
}

div.dist {
    padding-top: .3em;
}

div.ui-widget-content {
    background: #fff;
}

div.ui-widget-content a {
    color: #555 !important;
}

h2, h3, h4 {
    color: $primary-color;
    font-family: Arial, Sans-serif;
    line-height: 1.3;
    margin-top: 0;
}

img {
    border-style: none;
}

input {
    font-family: Arial, sans-serif;
    font-size: 16px;
}

pre {
    white-space: pre-wrap;
}

pre.log {
    background: #fff;
    color: #000;
    padding: 10px;
}

select [value^=l1] {
    color: green;
}

select [value^=l2] {
    color: blue;
}

select [value^=l3] {
    color: #FFC0CB;
}

select [value^=l4] {
    color: #A52A2A;
}

select [value^=l5] {
    color: #000;
}

select [value^=l6] {
    color: olive;
}

select [value^=l7] {
    color: gray;
}

select [value^=l8] {
    color: #FF0;
}

select.svgColumn {
    width: 50px;
}

select.svgRegexp {
    width: 120px;
}

select.svgSrc {
    width: 100px;
}

span.sort-item-delete-link {
    float: right;
    margin: 0 0 0 3px;
    padding: 0;
    vertical-align: middle;
}


ul.sortable-src li, ul.sortable-dest li {
    border-radius: 3px;
    color: #000 !important;
    font-size: .8em;
    line-height: 1.6em;
    margin: 3px;
    min-width: 120px;
    padding: 2px 2px 2px 4px;
    text-align: left;
    vertical-align: middle;
}

ul.sortable-src, ul.sortable-dest {
    background: #eee;
    border: 2px solid #000;
    border-radius: 3px;
    list-style-type: none;
    margin: 3px;
    min-height: 1.8em;
    min-width: 130px;
    padding: 2px;
    vertical-align: middle;
}